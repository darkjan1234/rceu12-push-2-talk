<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/button_borderless_red_pressed_dark"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="#ff000000" />
            <corners android:radius="@dimen/button_round_corner_radius" />
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_enabled="false">
                <shape android:shape="rectangle">
                    <stroke android:width="@dimen/button_round_border_width" android:color="@color/button_outline_red_disabled_dark" />
                    <corners android:radius="@dimen/button_round_corner_radius" />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <stroke android:width="@dimen/button_round_border_width" android:color="@color/button_outline_red_normal_dark" />
                    <corners android:radius="@dimen/button_round_corner_radius" />
                </shape>
            </item>
        </selector>
    </item>
</ripple>